{
    "name": "pretty-clever",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.9.13",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/jest": "^24.0.0",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "@types/recoil": "^0.0.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.1",
        "recoil": "^0.0.8",
        "typescript": "~3.7.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/react-hooks": "^3.2.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint-plugin-react-hooks": "^4.0.2",
        "husky": "^4.2.5",
        "react-test-renderer": "^16.13.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "echo commit params $HUSKY_GIT_PARAMS  stdin $HUSKY_GIT_STDIN",
            "pre-push": "read -ra huskyarr <<< \"$HUSKY_GIT_STDIN\"; echo huskyarr ${huskyarr[1]} ${huskyarr[3]}; diffout=$(git diff ${huskyarr[1]} ${huskyarr[3]} --name-only); diffline=$(${diffout/ /[$'\r\n']}); echo ${diffline};  yarn jest --findRelatedTests --forceExit --detectOpenHandles \"${diffline}\";"
        }
    }
}
